	<Cube name='CLB_Hospital_Cube' defaultMeasure='住院收费-住院登记'>
		<Dimensions>
			<Dimension source='收费项目'/>
			<Dimension source='收费项目大类'/>
			<Dimension source='临床科室'/>
			<Dimension source='医保类型'/>
			<Dimension source='员工'/>
			<Dimension source='日期'/>
			<Dimension source='收费确认状态'/>
			<Dimension source='收费记录状态'/>
			<Dimension source='时间计算同比'/>
			<Dimension source='时间计算维度'/>
			<Dimension source='药品'/>
			<Dimension source='系统类型'/>
		</Dimensions>
		<MeasureGroups>
			<MeasureGroup name='住院收费' table='OLAP_FACT_IPD_CHARGE'>
				<Measures>
					<Measure name='收费人次-住院收费' column='VISIT_NUMBER' aggregator='distinct-count' formatString='####0'/>
					<Measure name='住院项目品种-住院收费' column='CHARGE_ITEM_ID' aggregator='distinct-count' formatString='####0'/>
					<Measure name='住院收费-住院登记' column='CHARGE_ITEM_AMOUNT' aggregator='sum' formatString='####0.00'/>
					<Measure name='住院收费抗菌药品DDD' column='ANTIBIOTICS_DDD' aggregator='sum' formatString='####0.00'/>
					<Measure name='住院收费数量' column='CHARGE_ITEM_QUANTITY' aggregator='sum' formatString='####0.00'/>
				</Measures>
				<DimensionLinks>
					<ForeignKeyLink dimension='日期' foreignKeyColumn='CHARGE_DATE_ID'/>
					<ForeignKeyLink dimension='临床科室' foreignKeyColumn='CHARGE_DEP_ID'/>
					<ForeignKeyLink dimension='医保类型' foreignKeyColumn='INSURANCE_CATEGORY_ID'/>
					<ForeignKeyLink dimension='员工' foreignKeyColumn='POSTING_DOC_ID'/>
					<ForeignKeyLink dimension='收费项目' foreignKeyColumn='CHARGE_ITEM_ID'/>
					<ForeignKeyLink dimension='收费项目大类' foreignKeyColumn='ITEM_CATEGORY_ID'/>
					<ForeignKeyLink dimension='收费确认状态' foreignKeyColumn='CONFIRM_STATUS'/>
					<ForeignKeyLink dimension='收费记录状态' foreignKeyColumn='REC_STATUS'/>

					<ForeignKeyLink dimension='时间计算维度' foreignKeyColumn='PERIOD_ID'/>

					<ForeignKeyLink dimension='药品' foreignKeyColumn='DRUG_ITEM_ID'/>
					<ForeignKeyLink dimension='系统类型' foreignKeyColumn='TABLE_TYPE'/>
				</DimensionLinks>
			</MeasureGroup>
		</MeasureGroups>
		<CalculatedMembers>
			<CalculatedMember hierarchy="[时间计算维度].[时间计算维度层次结构]" name="同比">
				<Formula>
					<![CDATA[
            IIF(
                [日期].[年-半年-季-月-天].CurrentMember.Level IS [日期].[年-半年-季-月-天].[年]
                ,  Aggregate(CrossJoin({[时间计算维度].[时间计算维度].[1]}, ParallelPeriod([日期].[年-半年-季-月-天].[年],1,[日期].[年-半年-季-月-天].CurrentMember)))
                , IIF(
                      [日期].[年-半年-季-月-天].CurrentMember.Level IS [日期].[年-半年-季-月-天].[半年]
                      ,  Aggregate(CrossJoin({[时间计算维度].[时间计算维度].[1]}, ParallelPeriod([日期].[年-半年-季-月-天].[年],1,[日期].[年-半年-季-月-天].CurrentMember)))
                      ,IIF (
                      		[日期].[年-半年-季-月-天].CurrentMember.Level IS [日期].[年-半年-季-月-天].[季]
                      		,	 Aggregate(CrossJoin({[时间计算维度].[时间计算维度].[1]}, ParallelPeriod([日期].[年-半年-季-月-天].[年],1,[日期].[年-半年-季-月-天].CurrentMember)))
                      		,IIF (
                      			  [日期].[年-半年-季-月-天].CurrentMember.Level IS [日期].[年-半年-季-月-天].[月]
                      			  ,		Aggregate(CrossJoin({[时间计算维度].[时间计算维度].[1]}, ParallelPeriod([日期].[年-半年-季-月-天].[年],1,[日期].[年-半年-季-月-天].CurrentMember)))
                      			  ,	IIF (
                      			  		[日期].[年-半年-季-月-天].CurrentMember.Level IS [日期].[年-半年-季-月-天].[天]
                      			  		,  Aggregate(CrossJoin({[时间计算维度].[时间计算维度].[1]}, ParallelPeriod([日期].[年-半年-季-月-天].[年],1,[日期].[年-半年-季-月-天].CurrentMember)))
                      			  		,	null
                      			  		)
                      			  )
                      		)
                	  )
            	)
        			]]>
				</Formula>
			</CalculatedMember>
			<CalculatedMember name='上年_收费人次-住院收费' dimension='Measures' formatString='###0'>
				<Formula>AGGREGATE(PARALLELPERIOD([日期].[年-半年-季-月-日].[年],1,[日期].[年-半年-季-月-日].[年].currentmember),[Measures].[收费人次-住院收费])</Formula>
			</CalculatedMember>
			<CalculatedMember name='上年_住院项目品种-住院收费' dimension='Measures' formatString='###0'>
				<Formula>AGGREGATE(PARALLELPERIOD([日期].[年-半年-季-月-日].[年],1,[日期].[年-半年-季-月-日].[年].currentmember),[Measures].[住院项目品种-住院收费])</Formula>
